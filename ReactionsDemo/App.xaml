             xmlns:reactionsDemo="clr-namespace:ReactionsDemo">
	<Application.Resources>
		<ResourceDictionary>
			<ResourceDictionary.MergedDictionaries>
				<ResourceDictionary>
					<ResourceDictionary.MergedDictionaries>
						<ResourceDictionary Source="pack://application:,,,/Material;component/Themes/Generic.xaml"/>
						<!--<ResourceDictionary Source="pack://application:,,,/FlexTraveler;component/Themes/Material.xaml"/>-->
